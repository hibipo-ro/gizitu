<h1>編集画面</h1>

<%= link_to("一覧", "/") %>

<%= form_tag("/posts/#{@post.id}/update") do %>
  <input type="text" name="title" value="<%= @post.title %>">
  <input type="text" name="expect" value="<%= @post.expect %>">
  <input type="text" name="actual" value="<%= @post.actual %>">
  <input type="submit" value="編集">
<% end %>